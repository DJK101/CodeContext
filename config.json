{
  "server": {
    "port": 5000
  },
  "logging": {
    "console": {
      "enabled": true,
      "level": "DEBUG",
      "format": "%(levelname).1s:[%(name)s]> %(message)s",
      "date_format": "%YYYY-%M-%D"
    },
    "file": {
      "enabled": true,
      "level": "WARNING",
      "format": "%(asctime)s -%(levelname)s - %(name)s -  %(pathname)s:%(lineno)d - %(message)s",
      "date_format": "%YYYY-%m-%d",
      "output_dir": "logs",
      "filename": "app.log",
      "max_bytes": 10485760,
      "backup_count": 5
    },
    "db": {
      "enabled": true
    }
  }
}
