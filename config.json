{
  "server": {
    "port": 5000
  },
  "logging": {
    "console": {
      "enabled": true,
      "level": "DEBUG",
      "format": "%(levelname).1s:[%(name)s]> %(message)s",
      "date_format": "%YYYY-%M-%D"
    },
    "file": {
      "enabled": true,
      "level": "WARNING",
      "format": "%(levelname).1s:[%(name)s]> %(message)s",
      "date_format": "%YYYY-%M-%D",
      "output_dir": "logs",
      "filename": "app.log",
      "max_bytes": 10485760,
      "backup_count": 5
    }
  }
}
